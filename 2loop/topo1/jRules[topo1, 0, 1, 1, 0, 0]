{j[topo1, (n1_)?NonPositive, (n2_)?Positive, (n3_)?Positive, 
   (n4_)?NonPositive, (n5_)?NonPositive] :> 
  Expand[j[topo1, n5, 0, n1, n2, n3]/(j[topo1, -1, 0, 0, 0, 0] - 
      j[topo1, 0, -1, 0, 0, 0] + j[topo1, 0, 0, -1, 0, 0] - 
      j[topo1, 0, 0, 0, -1, 0] + j[topo1, 0, 0, 0, 0, -1] + 
      pp*j[topo1, 0, 0, 0, 0, 0])^n4]}
