{j[topo1, (n1_)?NonPositive, (n2_)?Positive, (n3_)?NonPositive, 
   (n4_)?Positive, (n5_)?NonPositive] :> 
  Expand[j[topo1, n5, n1, 0, n2, n4]/(j[topo1, -1, 0, 0, 0, 0] + 
      j[topo1, 0, -1, 0, 0, 0] - j[topo1, 0, 0, -1, 0, 0] + 
      j[topo1, 0, 0, 0, -1, 0] + j[topo1, 0, 0, 0, 0, -1] + 
      (2*m2 - pp)*j[topo1, 0, 0, 0, 0, 0])^n3]}
