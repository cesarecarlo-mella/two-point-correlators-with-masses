{{j[familyAllMass, (n1_)?Positive, (n2_)?Positive, (n3_)?NonPositive, 
    (n4_)?NonPositive, (n5_)?Positive] :> 
   Expand[j[familyAllMass, n1, n2, n3, n4, n5]]}, 
